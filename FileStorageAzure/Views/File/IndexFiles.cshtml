@model List<string><a asp-controller="File" asp-action="UploadFile">    Azure File</a><hr />@if (Model != null){    <ul class="list-group">        @foreach (string nombre in Model)        {            <li class="list-group-item">                @nombre                <a asp-controller="File" asp-action="ReadFile" asp-route-fileName="@nombre" class="btn btn-success">                    Read File                </a>                <a asp-controller="File" asp-action="DeleteFile" asp-route-fileName="@nombre" class="btn btn-danger">                    Delete File                </a>            </li>        }    </ul>}